        "editorBracketHighlight.foreground2": "#805AD5",
        "editorBracketHighlight.foreground3": "#48BB78",
        "editorBracketHighlight.foreground4": "#F56565",
        "editorBracketHighlight.foreground5": "#ECC94B",
        "editorBracketHighlight.foreground6": "#38B2AC",
        "editorBracketHighlight.unexpectedBracket.foreground": "#F56565",
        "editorUnnecessaryCode.opacity": "#A0AEC0",
        "editorGutter.addedBackground": "#48BB78",
        "editorGutter.modifiedBackground": "#ECC94B",
        "editorGutter.deletedBackground": "#F56565",
        "editorOverviewRuler.addedForeground": "#48BB78",
        "editorOverviewRuler.modifiedForeground": "#ECC94B",
        "editorOverviewRuler.deletedForeground": "#F56565",
        "editorOverviewRuler.errorForeground": "#F56565",
        "editorOverviewRuler.warningForeground": "#ECC94B",
        "editorOverviewRuler.infoForeground": "#4299E1",
        "editorOverviewRuler.bracketMatchForeground": "#4299E1"
    },
    "tokenColors": [
        {
            "name": "Keywords",
            "scope": ["keyword", "storage", "type", "modifier", "control"],
            "settings": {
                "foreground": "#805AD5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function", "variable.function"],
            "settings": {
                "foreground": "#4299E1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.template", "string.quoted"],
            "settings": {
                "foreground": "#48BB78"
            }
        },
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#A0AEC0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.other", "variable.parameter"],
            "settings": {
                "foreground": "#2D3748"
            }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
            "settings": {
                "foreground": "#ED8936"
            }
        },
        {
            "name": "Types",
            "scope": ["entity.name.type", "entity.name.class", "entity.name.interface", "entity.name.enum"],
            "settings": {
                "foreground": "#38B2AC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator", "keyword.operator.assignment", "keyword.operator.arithmetic", "keyword.operator.logical"],
            "settings": {
                "foreground": "#4A5568"
            }
        },
        {
            "name": "Decorators",
            "scope": ["meta.decorator", "meta.decorator punctuation.decorator"],
            "settings": {
                "foreground": "#805AD5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Unused",
            "scope": ["variable.unused", "parameter.unused"],
            "settings": {
                "foreground": "#A0AEC0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Deprecated",
            "scope": ["variable.deprecated", "function.deprecated", "class.deprecated"],
            "settings": {
                "foreground": "#F56565",
                "fontStyle": "italic strikethrough"
            }
        }
    ]
} 